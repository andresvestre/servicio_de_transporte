{
    "compilerOptions": {
      "baseUrl": ".",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "outDir": "dist",
      "resolveJsonModule": true,
      "rootDir": "src",
      "skipLibCheck": true,
      "sourceMap": true,
      "strict": true,
      "strictNullChecks": true,
      "target": "ES2021"
    },
    "include": [
      "src"
    ]
   }
   
   